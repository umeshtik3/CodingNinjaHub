{
"manifest_version": 2,
"name": "CodingNinjaHub",
"description": "Automatically integrate your Coding Ninja's submissions to GitHub Repo",
"version": "1.0",
"browser_action": {
"default_icon": "ICON (WITH EXTENSION)",
"default_popup": "LAYOUT HTML FILE"
},
"permissions": [
     "https://github.com/*",
    "https://api.github.com/*",
    "https://leetcode.com/*",
    "https://practice.geeksforgeeks.org/*",
    "tabs",
    "unlimitedStorage",
    "storage"
]
}